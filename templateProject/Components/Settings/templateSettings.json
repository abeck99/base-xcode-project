    "dataSourceCellXibFileLocation": "{{ projectName }}/TableViews/Xibs/{{ dataSource[\"uppercaseName\"] }}",
    "modelClassFileLocation": "{{ projectName }}/Models/{{ groupName }}",

    "controllersFileTemplates": {
        "filename": "className",
        "files": [
            "Templates/Controllers/XXX_Controllers.h",
            "Templates/Controllers/XXX_Controllers.m"
        ]
    },

    "controllerClassTemplates": {
        "filename": "className",
        "files": [
            "Templates/Controllers/XXX_ViewController.h",
            "Templates/Controllers/XXX_ViewController.m"
        ]
    },

    "controllerSectionTemplates": {
        "filename": "className",
        "files": [
            "Templates/Controllers/XXX_SectionViewController.h",
            "Templates/Controllers/XXX_SectionViewController.m"
        ]
    },
    
    "controllerXibTemplates": {
        "filename": "uppercaseName",
        "files": [
            "Templates/Controllers/BaseView.xib"
        ]
    },

    "controllerSectionXibTemplates": {
        "filename": "uppercaseName",
        "files": [
            "Templates/Controllers/SectionView.xib"
        ]
    },

    "popupClassTemplates": {
        "filename": "className",
        "files": [
            "Templates/Popups/Popup.h",
