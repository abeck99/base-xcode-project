{
    "controllersListFileLocation": "{{ projectName }}/MasterControllerList",
    "controllerClassFileLocation": "{{ projectName }}/Views/Controllers/{{ uppercaseName }}",
    "controllerXibFileLocation": "{{ projectName }}/Views/Xibs/{{ uppercaseName }}",
    "popupClassFileLocation": "{{ projectName }}/Popups/Controllers/{{ uppercaseName }}",
    "popupXibFileLocation": "{{ projectName }}/Popups/Xibs/{{ uppercaseName }}",
    "dataSourceClassFileLocation": "{{ projectName }}/TableViews/Controllers/{{ uppercaseName }}",
